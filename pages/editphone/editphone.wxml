 
 
 

<view class='login-ul'>
  <view class='login-cell'>
    <image  class='login-head-img' src='../../static/images/icon_phone.png' mode='aspectFit' />
    <input type='number' placeholder='请输入您的旧手机号'  value='{{phone}}' bindinput="bindinput"  ></input>

  </view>
  <view class='login-cell'>
    <image  class='login-head-img' src='../../static/images/icon_phone.png' mode='aspectFit' />
    <input  value='{{password}}' class='' type='number'  bindinput="bindinputPwd"   placeholder='请输入您的新手机号'></input>

  </view>
  
 
</view>

<view class='btn-without mt130 '>
  <button class='default' type="default" size="default" bindtap="login">确认修改 </button>

 </view>
